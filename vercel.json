{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://dev.api.bekindnetwork.com/api/v1/:match*"
    },
    {
      "source": "/auth/:match*",
      "destination": "https://dev.apinetbo.bekindnetwork.com/api/Authentication/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
